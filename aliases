# Copy these into your gitconfig, or include this file using:
# 
# [include]
# path = ~/repos/git_tools/aliases

[format]
pretty=%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)

[alias]
com=commit -am
m=merge
graph=log --graph

[gitflow "prefix"]
feature=feature

